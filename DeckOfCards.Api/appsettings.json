{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "Path": ".\\Logs\\DeckCardGameLog_Api.txt",
          "RollOnFileSizeLimit": "true",
          "FileSizeLimitBytes": "10485760",
          "RetainedFileCountLimit": "10"
        }
      }
    ]
  }
}
